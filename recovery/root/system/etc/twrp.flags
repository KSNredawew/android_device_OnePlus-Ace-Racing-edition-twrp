/my_company           erofs     /dev/block/by-name/my_company        flags=display="My Company";logical;backup=1;slotselect;ro
/my_carrier           erofs     /dev/block/by-name/my_carrier        flags=display="My Carrier";logical;backup=1;slotselect;ro
/my_region            erofs     /dev/block/by-name/my_region         flags=display="My Region";logical;backup=1;slotselect;ro

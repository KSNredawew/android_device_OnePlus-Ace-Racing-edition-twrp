# TWRP Configuration Flags for MediaTek MT6895/MT6779
# Mount point          fstype    device                                 flags

## Boot & Critical Partitions
/boot                 emmc      /dev/block/by-name/boot              flags=display="Boot";backup=1;flashimg=1;slotselect
/recovery             emmc      /dev/block/by-name/recovery          flags=display="Recovery";backup=1;flashimg=1
/dtbo                 emmc      /dev/block/by-name/dtbo              flags=display="DTBO";backup=1;flashimg=1;slotselect
/odmdtbo              emmc      /dev/block/by-name/odmdtbo           flags=display="ODM DTBO";backup=1;flashimg=1;slotselect
/boot_para            emmc      /dev/block/by-name/boot_para         flags=display="Boot Parameters"
/logo                 emmc      /dev/block/by-name/logo              flags=display="Logo";backup=1

## Security & Verification
/vbmeta               emmc      /dev/block/by-name/vbmeta            flags=display="VBMeta";slotselect
/vbmeta_system        emmc      /dev/block/by-name/vbmeta_system     flags=display="VBMeta System";slotselect
/vbmeta_vendor        emmc      /dev/block/by-name/vbmeta_vendor     flags=display="VBMeta Vendor";slotselect
/seccfg               emmc      /dev/block/by-name/seccfg            flags=display="Security Config"
/tee1                 emmc      /dev/block/by-name/tee1              flags=display="TEE1";slotselect
/tee2                 emmc      /dev/block/by-name/tee2              flags=display="TEE2";slotselect


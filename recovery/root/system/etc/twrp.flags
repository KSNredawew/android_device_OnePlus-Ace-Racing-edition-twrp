## Data & Cache
/data                 f2fs      /dev/block/by-name/userdata          flags=display="User Data";wipeingui;backup=1;fileencryption=aes-256-xts:aes-256-cts:v2+inlinecrypt_optimized;keydirectory=/metadata/vold/metadata_encryption
/cache                ext4      /dev/block/by-name/oplusreserve2     flags=display="Cache";wipeingui
/metadata             ext4      /dev/block/by-name/metadata          flags=display="Metadata";wipeingui

## Persistent & Misc
/misc                 emmc      /dev/block/by-name/misc              flags=display="Misc"
/nvram                emmc      /dev/block/by-name/nvram             flags=display="NVRAM"
/proinfo              emmc      /dev/block/by-name/proinfo           flags=display="Proinfo"

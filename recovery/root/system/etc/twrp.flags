# TWRP Configuration Flags for MediaTek MT6895/MT6779
# Mount point          fstype    device                                 flags

## Boot & Critical Partitions
/boot                 emmc      /dev/block/by-name/boot              flags=display="Boot";backup=1;flashimg=1;slotselect
/vendor_boot          emmc      /dev/block/by-name/vendor_boot       flags=display="Vendor Boot";backup=1;flashimg=1;slotselect
/dtbo                 emmc      /dev/block/by-name/dtbo              flags=display="DTBO";backup=1;flashimg=1;slotselect
/odmdtbo              emmc      /dev/block/by-name/odmdtbo           flags=display="ODM DTBO";backup=1;flashimg=1;slotselect
/boot_para            emmc      /dev/block/by-name/boot_para         flags=display="Boot Parameters";backup=0
/logo                 emmc      /dev/block/by-name/logo              flags=display="Logo";backup=1

## Security & Verification
/vbmeta               emmc      /dev/block/by-name/vbmeta            flags=display="VBMeta";backup=1;flashimg=1;slotselect
/vbmeta_system        emmc      /dev/block/by-name/vbmeta_system     flags=display="VBMeta System";backup=1;slotselect
/vbmeta_vendor        emmc      /dev/block/by-name/vbmeta_vendor     flags=display="VBMeta Vendor";backup=1;slotselect
/seccfg               emmc      /dev/block/by-name/seccfg            flags=display="Security Config";backup=0
/tee1                 emmc      /dev/block/by-name/tee1              flags=display="TEE1";backup=0;slotselect
/tee2                 emmc      /dev/block/by-name/tee2              flags=display="TEE2";backup=0;slotselect


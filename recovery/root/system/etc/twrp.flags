## Data & Cache
/data                 f2fs      /dev/block/by-name/userdata          flags=display="User Data";wipeingui;backup=1;fileencryption=aes-256-xts:aes-256-cts:v2+inlinecrypt_optimized;keydirectory=/metadata/vold/metadata_encryption;fsverity;fscompress
/cache                ext4      /dev/block/by-name/oplusreserve2     flags=display="Cache";wipeingui;backup=0
/metadata             ext4      /dev/block/by-name/metadata          flags=display="Metadata";wipeingui;backup=0

## Persistent & Misc
/misc                 emmc      /dev/block/by-name/misc              flags=display="Misc";backup=0
/nvram                emmc      /dev/block/by-name/nvram             flags=display="NVRAM";backup=0
/proinfo              emmc      /dev/block/by-name/proinfo           flags=display="Proinfo";backup=0
/frp                  emmc      /dev/block/by-name/frp               flags=display="FRP";backup=0
/expdb                emmc      /dev/block/by-name/expdb             flags=display="EXPDB";backup=0
/oplusreserve         ext4      /dev/block/by-name/oplusreserve2     flags=display="OPPO Reserve";backup=0
/cdt_engineering      emmc      /dev/block/by-name/cdt_engineering   flags=display="CDT Engineering";backup=0

## Vendor Mounts
/protect_f            ext4      /dev/block/by-name/protect1          flags=display="Protect F";backup=0
/protect_s            ext4      /dev/block/by-name/protect2          flags=display="Protect S";backup=0
/nvdata               ext4      /dev/block/by-name/nvdata            flags=display="NV Data";backup=0
/nvcfg                ext4      /dev/block/by-name/nvcfg             flags=display="NV Config";backup=0

## External Storage
/external_sd          vfat      /dev/block/mmcblk0p1                 flags=display="SD Card";storage;removable;wipeingui
/usb_otg              vfat      /dev/block/sdd1                      flags=display="USB OTG";storage;removable;wipeingui
